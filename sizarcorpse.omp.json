{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",

  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "#0f172a",
          "style": "powerline",
          "powerline_symbol": "",
          "template": " ðŸ’€ ",
          "type": "os"
        },
        {
          "background": "#0f172a",
          "style": "powerline",
          "powerline_symbol": "",
          "template": "{{ .UserName }}",
          "type": "session"
        },
        {
          "background": "#111827",
          "style": "powerline",
          "powerline_symbol": "",
          "properties": {
            "style": "folder"
          },
          "template": " ðŸ“ƒ {{ .Path }} ",
          "type": "path"
        },
        {
          "type": "node",
          "style": "powerline",
          "background": "#059669",
          "properties": {
            "fetch_version": false
          },
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}"
        },
        {
          "type": "git",
          "style": "powerline",
          "powerline_symbol": "",
          "foreground": "#0f172a",
          "background": "#ffeb3b",
          "background_templates": [
            "{{ if (.Working.Changed) }}#7dd3fc{{ end }}",
            "{{ if (.Staging.Changed) }}#5eead4{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#fde047{{ end }}",
            "{{ if gt .Ahead 0 }}#86efac{{ end }}",
            "{{ if gt .Behind 0 }}#fca5a5{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ðŸ“ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ðŸš€ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ðŸ’¾ {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "î‚ ",
            "branch_identical_icon": "â‡”",
            "branch_ahead_icon": "â†‘",
            "branch_behind_icon": "â†“",
            "branch_gone_icon": "ðŸ¤–",
            "display_status": true,
            "status_separator_icon": "|",
            "local_working_icon": "âš™",
            "local_staged_icon": "âœ“",
            "commit_icon": "ðŸ“„",
            "rebase_icon": "â™»ï¸",
            "tag_icon": "ðŸ·ï¸",
            "cherry_pick_icon": "ðŸ’",
            "merge_icon": "âš¡",
            "working_color": "#ff0000",
            "staging_color": "#16a34a",
            "status_colors_enabled": true,
            "color_background": false,
            "prefix": " ",
            "postfix": "",
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            }
          }
        },
        {
          "background": "#e11d48",
          "foreground": "#ffffff",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "style": "diamond",
          "template": " \ueba2 {{ .FormattedMs }}\u2800",
          "type": "executiontime"
        }
      ],
      "type": "prompt"
    }
  ],
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#ffffff",
    "template": "-> "
  },
  "final_space": true,
  "version": 2
}
